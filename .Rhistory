age = c( 6, 7, 8.5, 6, 5, 16, 17, 16, 45, 77 ),
value_2_a = c( 7, 23, 22, 6, 23, 45, 8, 21, 66, 42 ),
value_1_b = c( 12, 392, 18, 11, 344, 414, 15, 110, 222, 231 ),
value_2_b = c( 47, 223, 24, 26, 213, 415, 84, 421, 266, 442 ) )
age_gender = function( subjects_data, group_by = NULL ) {
s_dat = subjects_data
if ( is.null( group_by ) ) {
s_dat$neat_cond = 99
} else if ( class(group_by) == "character") {
s_dat$neat_cond = s_dat[[ group_by ]]
} else {
s_dat$neat_cond = group_by
}
for_gender = s_dat[,c("neat_cond","gender")]
gender = data.frame(prop.table(table(for_gender), 1))
print(gender)
gender = head(gender, nrow(gender)/2 )
age = do.call(data.frame, aggregate( s_dat$age, by = list( s_dat$neat_cond ), function(x) c(count = length(x), mean = mean(x), sd = sd(x))) )
names(age)[names(age) == "Group.1"] <- "neat_cond"
age_gend = merge( age, gender, by = 'neat_cond' )
for(i in 1:nrow(age_gend)) {
row <- age_gend[i,]
prnt( 'Group < ', row[[1]], ' >: ', row[2], ' subjects (age = ', ro(row[3],1), 'CHAR_PLUSMIN', ro(row[4],1), ', ', ro(row[6]*100,1), "% male)", sep = "")
}
}
age_gender( ex2, 'group' )
age_gender( ex2, ex2$group )
age_gender( ex2 )

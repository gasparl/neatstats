prnt = function( ... ) {
to_print = gsub('-', 'CHAR_MINUS', paste0( ... ) )
to_print = sub("e\\+0*", " CHAR_X 10^", to_print )
to_print = gsub("p = 0.", "p = .", to_print )
to_print = gsub("p = .000", "p < .001", to_print )
change_pairs = list( c('CHAR_MINUS', '\u2013'),
c('CHAR_PLUSMIN', '\u00b1'),
c('CHAR_X', '\u00d7'),
c('CHAR_ETA', '\u03b7'),
c('CHAR_EPS', '\u03b5') )
Encoding(to_print) = "UTF-8"
for ( pair in change_pairs ) {
to_print = gsub( pair[1], pair[2], to_print )
Encoding(to_print) = "UTF-8"
}
printing( to_print )
}
printing = function( to_print ) {
cat( to_print, fill = T)
}
age_gender = function( subjects_data, group_by = NULL ) {
s_dat = subjects_data
if ( is.null( group_by ) ) {
s_dat$neat_cond = 99
} else if ( class(group_by) == "character") {
s_dat$neat_cond = s_dat[[ group_by ]]
} else {
s_dat$neat_cond = group_by
}
for_gender = s_dat[,c("neat_cond","gender")]
gender = data.frame(prop.table(table(for_gender), 1))
gender = head(gender, nrow(gender)/2 )
age = do.call(data.frame, aggregate( s_dat$age, by = list( s_dat[[neat_cond]] ), function(x) c(count = length(x), mean = mean(x), sd = sd(x))) )
names(age)[names(age) == "Group.1"] <- "neat_cond"
age_gend = merge( age, gender, by = neat_cond )
for(i in 1:nrow(age_gend)) {
row <- age_gend[i,]
prnt( 'condition ', row[[1]], ': count ', ro(row[2],1), ', age = ', ro(row[3],1), 'CHAR_PLUSMIN', ro(row[4],1), ', male ', ro(row[6]*100,1), "%", sep = "")
}
}
